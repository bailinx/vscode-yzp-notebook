{
  "name": "vscode-yzp-notebook",
  "displayName": "Pomelo Peel Markdown Notebook",
  "description": "Pomelo Peel Markdown Notebook For VSCode",
  "version": "0.0.1",
  "publisher": "baili",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [],
    "viewsContainers": {
      "activitybar": [{
        "id": "pomeloPeel",
        "title": "柚子皮笔记本",
        "icon": "images/notebook_30x30.svg"
      }]
    },
    "views": {
      "pomeloPeel": [{
        "id": "pomeloPeelLocal",
        "name": "我的笔记"
      }]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}